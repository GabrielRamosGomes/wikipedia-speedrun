{
  "name": "wikipedia-speedrun",
  "version": "1.0.0",
  "description": "A Wikipedia speedrunning game where players navigate from one article to another using only the links within the articles.",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "bin": {
    "wikipedia-speedrun": "dist/cli.js"
  },
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "typecheck": "tsc --build",
    "play": "pnpm --filter @wiki-speedrun/cli play",
    "dev:cli": "pnpm --filter @wiki-speedrun/cli dev"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
